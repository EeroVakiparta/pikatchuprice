# PikatchuPrice environment variables
# Copy this file to .env and fill in your own values

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# GitHub Personal Access Token (for semantic-release)
# Generate this at: https://github.com/settings/tokens
# Needs 'repo' scope for private repos or 'public_repo' for public repos
NPM_TOKEN=your_github_personal_access_token 