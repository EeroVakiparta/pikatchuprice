# PikatchuPrice environment variables
# Copy this file to .env and fill in your own values

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# GitHub Personal Access Token (for semantic-release)
# Generate this at: https://github.com/settings/tokens
# Needs 'repo' scope for private repos or 'public_repo' for public repos
NPM_TOKEN=your_github_personal_access_token

# Example environment variables for the project

# AWS credentials
AWS_PROFILE=
AWS_REGION=eu-north-1
AWS_ACCOUNT_ID=12345678912

# GitHub creds if needed
GITHUB_TOKEN=

# DB name
TABLE_NAME=

# API
API_BASE_URL=https://example.execute-api.eu-north-1.amazonaws.com/prod/

# Tankille API credentials
TANKILLE_EMAIL=your.email@example.com
TANKILLE_USERNAME=your_username
TANKILLE_PASSWORD=your_password
FUEL_API_GATEWAY_URL=https://example.execute-api.eu-north-1.amazonaws.com/prod 